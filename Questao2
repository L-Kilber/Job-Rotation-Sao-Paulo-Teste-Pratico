def Fibonacci(valor_inicial):
    vr = valor_inicial
    i = 0
    v1 = 0
    v2 = 1
    v3 = 1
    while i <= vr:
        i += 1
        if vr == v1 or vr == v2 or vr == v3:
            print("Pertence a sequência")
            break
        else:
            v3 = v1 + v2
            v1 = v2
            v2 = v3
            if i == vr:
                print("Não pertence a sequência")
        
while True:
  valor_inicial = int(input("Digite um valor: "))  
  Fibonacci(valor_inicial)
